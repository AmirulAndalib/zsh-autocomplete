#autoload

zstyle -t :autocomplete:tab: insert-unambiguous &&
  return 1
(( compstate[nmatches] > 1 )) ||
  return 1
[[ $IPREFIX$PREFIX$SUFFIX$ISUFFIX == *$compstate[unambiguous]* ]] &&
  return 1

local tag=unambiguous
_tags $tag
_tags ||
  return 1
_requested $tag ||
  return 1

_message -e unambiguous "common substring:%b %S$compstate[unambiguous]%s"
